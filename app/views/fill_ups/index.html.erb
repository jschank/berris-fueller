<h1>Listing fill_ups</h1>

<table>
  <tr>
    <th>Brand</th>
    <th>Grade</th>
    <th>Odometer</th>
    <th>Cost in cents</th>
    <th>Gallons</th>
    <th>Date</th>
  </tr>

<% for fill_up in @fill_ups %>
  <tr>
    <td><%=h fill_up.brand %></td>
    <td><%=h fill_up.grade %></td>
    <td><%=h fill_up.odometer %></td>
    <td><%=h fill_up.cost_in_cents %></td>
    <td><%=h fill_up.gallons %></td>
    <td><%=h fill_up.date %></td>
    <td><%= link_to 'Show', fill_up %></td>
    <td><%= link_to 'Edit', edit_fill_up_path(fill_up) %></td>
    <td><%= link_to 'Destroy', fill_up, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New fill_up', new_fill_up_path %>
